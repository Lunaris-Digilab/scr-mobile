{
  "expo": {
    "name": "glowist",
    "slug": "glowist",
    "scheme": "glowist",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.glowist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.glowist"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.838895646158-uqtknhlnhga5vmqa6n6pn0u97qslb7en"
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0ba9c574-6405-4030-89cb-db7504b7b784"
      }
    },
    "owner": "lunarisdigilab"
  }
}
